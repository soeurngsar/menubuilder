{
    "name": "soeurngsar/menubuilder",
    "description": "drag and drop menu generator like wordpress for laravel backpack",
    "keywords": ["Laravel", "Menu", "Drag Drop", "Builder","Builder", "Backpack"],
    "authors": [
    	{
            "name": "Sar Soeurng",
            "email": "sar.soeurng@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Cristian Garcia",
            "email": "informacion@cristiangarcia.co",
            "role" : "Original Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "backpack/crud": "4.1.*"
    },
    "autoload": {
        "psr-4": {
          "SoeurngSar\\MenuBuilder\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoeurngSar\\MenuBuilder\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "SoeurngSar\\MenuBuilder\\app\\Facades\\Menu"
            }
        }
    }
}
